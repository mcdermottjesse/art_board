<h1>Add new Art</h1>


<%= form_for :art, url: art_index_path do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :url %><br>
    <%= f.url_field :image %>
  </p>

  <p>
    <%= f.label :description %><br>
    <%= f.text_field :description %>
  </p>

  <p>
    <%= f.submit %>
  </p>

<% end %>

<% if flash[:danger] %>
<div class="create_err"><%= flash[:danger] %></div>
<% end %>

