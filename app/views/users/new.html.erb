

<h1 class="signup_title">Sign Up</h1>
<div class="user_signup">
<%= form_for :user, url: '/users' do |f|%>
  <%= f.label :username%><br>
  <%= f.text_field :username%><br>
  <%= f.label :password%><br>
  <%= f.password_field :password%><br>
  <%= f.submit %>
<%end%>
</div>
<% if flash[:danger] %>
<div class="signup_msg"><%= flash[:danger] %></div>
<% end %>

